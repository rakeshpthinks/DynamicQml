{
    "Window": {
        "width": 800,
        "height": 600,
        "title": "Dynamic Application",
        "tabs": [
            {
                "title": "Slider Tab",
                "layout": "CustomColumnLayout",
                "layoutProperties": {
                    "spacing": 20,
                    "backgroundColor": "#f0f0f0",
                    "borderWidth": 2,
                    "borderColor": "black"
                },
                "components": [
                    {
                        "type": "CustomSlider",
                        "properties": {
                            "minimum": 0,
                            "maximum": 100,
                            "value": 30,
                            "stepSize": 5,
                            "trackColor": "lightgrey",
                            "progressColor": "green",
                            "handleColor": "red"
                        }
                    },
                    {
                        "type": "Text",
                        "properties": {
                            "text": "Slider Value",
                            "font.pixelSize": 16,
                            "color": "blue"
                        }
                    }
                ]
            },
            {
                "title": "Progress Tab",
                "layout": "CustomRowLayout",
                "layoutProperties": {
                    "spacing": 15,
                    "backgroundColor": "#e0e0e0",
                    "borderWidth": 1,
                    "borderColor": "gray"
                },
                "components": [
                    {
                        "type": "CustomProgressBar",
                        "properties": {
                            "minimumValue": 0,
                            "maximumValue": 100,
                            "value": 70,
                            "trackColor": "gray",
                            "progressColor": "blue"
                        }
                    },
                    {
                        "type": "Switch",
                        "properties": {
                            "checked": true

                        }
                    }
                ]
            },
            {
                "title": "Input Tab",
                "layout": "CustomGridLayout",
                "layoutProperties": {
                    "columns": 2,
                    "rowSpacing": 10,
                    "columnSpacing": 15,
                    "backgroundColor": "#dff2ff",
                    "borderWidth": 3,
                    "borderColor": "blue"
                },
                "components": [
                    {
                        "type": "TextField",
                        "properties": {
                            "placeholderText": "Enter your name",
                            "width": 200
                        }
                    },
                    {
                        "type": "Button",
                        "properties": {
                            "text": "Submit"

                        }
                    }
                ]
            }
        ]
    }
}
